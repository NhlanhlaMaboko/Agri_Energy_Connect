@model IEnumerable<AgriEnergy_Connect.Models.Product>

<h2>All Products</h2>

<table class="table">
    <thead>
        <tr>
            <th>Name</th>
            <th>Price</th>
            <th>Farmer</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var product in Model)
        {
            <tr>
                <td>@product.Name</td>
                <td>@product.Price</td>
                <td>@product.Farmer?.ApplicationUser?.UserName</td>
            </tr>
        }
    </tbody>
</table>
